  "author": "Jeff <jeff@jeffjewiss.com>",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "2.11.1",
    "devtool": "1.8.2",
    "gulp": "3.9.1",
    "gulp-plumber": "1.1.0",
    "gulp-postcss": "6.1.0",
    "gulp-util": "3.0.7",
    "postcss-discard-comments": "2.0.4"
  }
}
